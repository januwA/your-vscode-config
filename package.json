{
	"name": "your-vscode-config",
	"displayName": "your vscode config",
	"description": "保存您vscode的`settings.json`和`extensions`用于在多台设备上同步",
	"publisher": "Ajanuw",
	"icon": "images/logo.png",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cowsay.config"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cowsay.config",
				"title": "your vscode config"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/januwA/your-vscode-config"
	},
	"license": "MIT",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/glob": "^7.1.1",
		"@types/json5": "0.0.30",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/shelljs": "^0.8.6",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"fs-extra": "^8.1.0",
		"shelljs": "^0.8.5",
		"json5": "^2.1.1"
	}
}
